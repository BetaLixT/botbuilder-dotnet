{
  "$schema": "../../../tests.schema",
  "$kind": "Microsoft.Test.Script",
  "dialog": {
    "$kind": "Microsoft.AdaptiveDialog",
    "triggers": [
      {
        "$kind": "Microsoft.OnBeginDialog",
        "actions": [
          {
            "$kind": "Microsoft.SetProperty",
            "property": "dialog.events",
            "value": "=['event1', 'event2']"
          },
          {
            "$kind": "Microsoft.Foreach",
            "itemsProperty": "dialog.events",
            "actions": [
              {
                "$kind": "Microsoft.SendActivity",
                "activity": "before event: ${dialog.foreach.value}"
              },
              {
                "$kind": "Microsoft.EmitEvent",
                "eventName": "=dialog.foreach.value"
              },
              {
                "$kind": "Microsoft.SendActivity",
                "activity": "after event: ${dialog.foreach.value}"
              }
            ]
          }
        ]
      },
      {
        "$kind": "Microsoft.OnDialogEvent",
        "event": "event1",
        "actions": [
          {
            "$kind": "Microsoft.SendActivity",
            "activity": "CustomEvent event1 fired"
          }
        ]
      },
      {
        "$kind": "Microsoft.OnDialogEvent",
        "event": "event2",
        "actions": [
          {
            "$kind": "Microsoft.SendActivity",
            "activity": "CustomEvent event2 fired"
          }
        ]
      }
    ],
    "autoEndDialog": true,
    "defaultResultProperty": "dialog.result"
  },
  "script": [
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "before event: event1"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "CustomEvent event1 fired"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "after event: event1"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "before event: event2"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "CustomEvent event2 fired"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "after event: event2"
    }
  ]
}